pid file=/var/run/rsyncd.pid
port=7999

# MODULE OPTIONS
[sidestream]
    comment = Sidestream data from a conventional webserver.
    path = RSYNCDIR_SS
    list = yes
    read only = yes
    transfer logging = no
    ignore errors = no
    ignore nonreadable = yes

[npad]
    comment = NPAD data.  See http://www.psc.edu/networking/projects/pathdiag/.
    path = RSYNCDIR_NPAD
    list = yes
    read only = yes
    transfer logging = no
    ignore errors = no
    ignore nonreadable = yes

[paris-traceroute]
    comment = Paris traceoute traces.
    path = RSYNCDIR_PTR
    list = yes
    read only = yes
    transfer logging = no
    ignore errors = no
    ignore nonreadable = yes
